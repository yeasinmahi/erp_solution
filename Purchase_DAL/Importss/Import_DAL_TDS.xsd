              <DbSource ConnectionRef="ERP_InventoryConnectionString (Settings)" DbObjectName="ERP_Inventory.dbo.tblRFQMain" DbObjectType="Table" GenerateMethods="Get" GenerateShortCommands="false" GeneratorGetMethodName="IndentStatusViewGetData" GetMethodModifier="Public" GetMethodName="IndentStatusViewGetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="IndentStatusViewGetData" UserSourceName="IndentStatusViewGetData">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT r.intRFQID, r.ysnComplete,stuff((SELECT ', ' + CAST(rd.intIndentID AS VARCHAR(20)) FROM ERP_Inventory.dbo.tblRFQDetail rd WHERE rd.intRFQID=r.intRFQID for xml path('')), 1, 1, '') AS strIndent,
r.dteRFQDate, e.strEmployeeName+ ' ['+ e.strDesignation + ']' AS strCreateBy, COUNT(DISTINCT(quot.intSupplierID)) AS numQuote 
FROM ERP_Inventory.dbo.tblRFQMain r JOIN ERP_HR.dbo.QryEmployeeProfileAll e ON r.intInsertBy=e.intEmployeeID 
LEFT JOIN ERP_Inventory.dbo.tblQuotation quot ON r.intRFQID=quot.intRFQID 
WHERE r.intUnitID=@unit  AND ysnComplete=@type
GROUP BY dteRFQDate, r.intRFQID, e.strEmployeeName, e.strDesignation,r.ysnComplete
HAVING COUNT(DISTINCT(quot.intSupplierID))&gt;0
ORDER BY dteRFQDate </CommandText>
