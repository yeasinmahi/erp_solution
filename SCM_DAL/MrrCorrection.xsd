                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select mrd.intItemID,(i.strItemName+i.strDescription+i.strPartNo) as Item,i.struom,mrd.numPOQty,mrd.numReceiveQty,mrd.monBDTTotal 
from ERP_Inventory.dbo.tblFactoryReceiveMRR mrr 
