                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT dteTransactionDate, dteReceiveDate, strChillingCenterName, strSupplierName, strIssueChallanNo, cast(Isnull(sum(intOutQty),0) as decimal(18,2)) as ReceiveQty, 
cast(intOutFatPercent as decimal(18,2)) as intOutFatPercent, cast(monOutRate as decimal(18,2)) as monOutRate, cast(Isnull(sum(monOutValue),0) as decimal(18,2)) as RecValue,'' as ReceQty,'','','', iss.intChillingCenterID, iss.intSupplierID, iss.strZoneName, intIssueID
FROM AG_DairyMilk.dbo.tblDairyMilkIssue iss
LEFT OUTER JOIN AG_Global.dbo.tblChillingCenter cc ON iss.intChillingCenterID = cc.intChillingCenterID 
LEFT OUTER JOIN AG_Global.dbo.tblAG_Supplier sup ON sup.intSupplierID = iss.intSupplierID Where iss.intVehicleID=335 and iss.ysnMRRComplete=0 
and cast(iss.dteTransactionDate as date)=@dteDate and iss.intChillingCenterID=@intCCID
Group By strIssueChallanNo, dteTransactionDate, dteReceiveDate, strChillingCenterName, strSupplierName, iss.intChillingCenterID, 
intOutFatPercent, monOutRate, iss.intSupplierID, iss.strZoneName, intIssueID  Order by strIssueChallanNo</CommandText>
