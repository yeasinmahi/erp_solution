          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="GetDataForMRRTableAdapter" GeneratorDataComponentClassName="GetDataForMRRTableAdapter" Name="GetDataForMRR" UserDataComponentName="GetDataForMRRTableAdapter">
