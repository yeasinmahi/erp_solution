              <DbSource ConnectionRef="ERP_HRConnectionString (Settings)" DbObjectName="ERP_HR.dbo.tblBenifitDetails" DbObjectType="Table" GenerateMethods="Get" GenerateShortCommands="false" GeneratorGetMethodName="GetAttendanceBenifitData" GetMethodModifier="Public" GetMethodName="GetAttendanceBenifitData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetAttendanceBenifitData" UserSourceName="GetAttendanceBenifitData">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>Select bnft.intRow, pro.strEmployeeName +' ['+ pro.strEmployeeCode +']'+' ['+ pro.strJobStationName +']' 
+' ['+ cast(bnft.numAmount as varchar(100)) +'] for ' + btp.strBenifit as Descriptions
from erp_hr.dbo.tblBenifitDetails bnft inner join erp_hr.dbo.tblBenifitType btp on bnft.intBenifit=btp.intBenifit
inner join erp_hr.dbo.QryEmployeeProfileAll pro on bnft.intEnroll=pro.intEmployeeID and 
bnft.ysnActive=1 where pro.intJobStationID in (Select intJobStationID from erp_hr.dbo.tblEmployeeJobStationOperator where intEmployeeId=@userid) 
Order by cast(bnft.dtAction as date) desc, pro.strEmployeeCode asc</CommandText>
