          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="TblAttendanceBenifitTableAdapter" GeneratorDataComponentClassName="TblAttendanceBenifitTableAdapter" Name="TblAttendanceBenifit" UserDataComponentName="TblAttendanceBenifitTableAdapter">
